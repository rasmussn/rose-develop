START
TYPE TABLE'TYPE'1 TABLE W 27;
  BEGIN
    ITEM TABLE'ITEM'2 U 6 POS(0,21);
  END

DEF TABLE TABLE'1 (0:10) TABLE'TYPE'1;

DEF PROC PROC'1 RENT;
  BEGIN
    ITEM ITEM'1 P TABLE'TYPE'1;
    FOR J : 1 BY 1 WHILE J <= 10;
      BEGIN
       TABLE'ITEM'2 @ ITEM'1 = TABLE'ITEM'2 @ (LOC(TABLE'1(J-1)));
      END
  END
TERM
