START

CONSTANT ITEM ITEM'1 U 4 = 10;

DEF TABLE TABLE'1 (0:10);
  BEGIN
   ITEM ITEM'2 U 6;
  END

TYPE TABLE'TYPE'1 TABLE W 27;
  BEGIN
    ITEM ITEM'3 U 6 POS(1,1);
  END

DEF TABLE TABLE'2 (0:10) TABLE'TYPE'1;

DEF TABLE TABLE'3 (0:10);
  BEGIN
   ITEM ITEM'4 U 6;
  END

DEF PROC PROC'1 RENT;
  BEGIN
   FOR I : 0 BY 1 WHILE (I <= ITEM'1);
      BEGIN
       ITEM'2(I) = ITEM'3 @
           (LOC(TABLE'2(ITEM'4(I))));
      END
  END
TERM
